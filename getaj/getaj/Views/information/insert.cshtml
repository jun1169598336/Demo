                    @Html.ValidationMessageFor(model => model.infoImg, "", new { @class = "text-danger" })
                </div>
            </div>
    
            <div class="form-group">
                @Html.LabelFor(model => model.typeid, htmlAttributes: new { @class = "control-label col-md-2" })
